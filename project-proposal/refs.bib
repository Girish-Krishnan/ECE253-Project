@article{SAPUTRA2024404,
title = {Low Light Image Enhancement in License Plate Recognition using URetinex-Net and TRBA},
journal = {Procedia Computer Science},
volume = {234},
pages = {404-411},
year = {2024},
note = {Seventh Information Systems International Conference (ISICO 2023)},
issn = {1877-0509},
doi = {https://doi.org/10.1016/j.procs.2024.03.021},
url = {https://www.sciencedirect.com/science/article/pii/S1877050924003788},
author = {Vriza Wahyu Saputra and Nanik Suciati and Chastine Fatichah},
keywords = {Low Light Image Enhancement (LLIE), URetinex-Net, Automatic License Plate Recognition (ALPR), TRBA},
abstract = {The license plate recognition system currently in use is susceptible to interference from the external environment and performs poorly in low-light conditions. This paper presents a solution for license plate recognition under a low-light environment. We adopted URetinex-Net methods that unfold an optimization issue into a learnable network to decompose a low illumination image into reflectance and illumination layers. We also adopted TRBA, an end-to-end recognition method involving no character segmentation. The experimental results show that the accuracy of the night environment of the proposed method is 80.11% increased by 5.11% compared to without the low light image enhancement method.}
}

